				<key>ActionParameters</key>
				<dict>
					<key>COMMAND_STRING</key>
					<string>cd "$1"
mkdir -p /tmp/DUALISO

#build new input path mlvfs_workflow
if ls /tmp/DUALISO/mlvfs_path
then
rm /tmp/DUALISO/mlvfs_path
echo "$1" &gt; /tmp/DUALISO/path_1
fi

#choose new output
if ls /tmp/DUALISO/OUT_path
then
rm /tmp/DUALISO/OUT_path
echo "$1" &gt; /tmp/output
open "$(cat /tmp/DUALISO/path_2)"Menu.command
fi

#select a darkframe folder
if ls /tmp/DARK_FOLDER_path
then
rm /tmp/DARK_FOLDER_path
echo "$1" &gt; /tmp/DARK_FOLDER
fi

#select afplay folder
if ls /tmp/afplay
then
rm /tmp/afplay
echo "$1" &gt; /tmp/afplay_path
fi

if ls /tmp/DUALISO/RAW_OUTPUT
then
echo "$1" &gt; /tmp/DUALISO/RAW_OUTPUT
rm /tmp/DUALISO/RAW_demolish
fi

#output MLV compressor(lj92)
if ls /tmp/DUALISO/MLV_cprs_out
then
echo "$1" &gt; /tmp/DUALISO/MLV_cprs_output
rm /tmp/DUALISO/MLV_cprs_out
fi

</string>
